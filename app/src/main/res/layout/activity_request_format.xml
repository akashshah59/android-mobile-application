<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.aakash.iot.RequestFormat"
    tools:layout_editor_absoluteY="81dp"
    tools:layout_editor_absoluteX="0dp">


    <TextView
        android:id="@+id/textView"
        android:layout_width="79dp"
        android:layout_height="49dp"
        android:layout_marginBottom="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginTop="36dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="LAMP_0"
        app:layout_constraintBottom_toTopOf="@+id/lamp_1"
        app:layout_constraintHorizontal_bias="0.25"
        app:layout_constraintLeft_toLeftOf="@+id/lamp_0"
        app:layout_constraintRight_toRightOf="@+id/lamp_0"
        app:layout_constraintTop_toBottomOf="@+id/lamp_0"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/editText3"
        android:layout_width="79dp"
        android:layout_height="49dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="LAMP_1"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="74dp"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="@+id/lamp_1"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="@+id/lamp_1"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/lamp_1"
        app:layout_constraintHorizontal_bias="0.25"
        app:layout_constraintVertical_bias="0.0" />

    <ImageButton
        android:id="@+id/lamp_1"
        android:layout_width="99dp"
        android:layout_height="126dp"
        app:srcCompat="@drawable/btn_off"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="76dp"
        android:layout_marginLeft="312dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintHorizontal_bias="0.625" />

    <ImageButton
        android:id="@+id/lamp_all"
        android:layout_width="98dp"
        android:layout_height="140dp"
        app:srcCompat="@drawable/btn_off"
        android:layout_marginRight="9dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="8dp"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="8dp"
        app:layout_constraintVertical_bias="0.458" />

    <ImageButton
        android:id="@+id/lamp_0"
        android:layout_width="99dp"
        android:layout_height="142dp"
        app:srcCompat="@drawable/btn_off"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="0dp"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintHorizontal_bias="1.0" />

    <TextView
        android:id="@+id/editText"
        android:layout_width="79dp"
        android:layout_height="49dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginTop="19dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="LAMP_ALL"
        app:layout_constraintHorizontal_bias="0.666"
        app:layout_constraintLeft_toLeftOf="@+id/lamp_all"
        app:layout_constraintRight_toRightOf="@+id/lamp_all"
        app:layout_constraintTop_toBottomOf="@+id/lamp_all" />

    <ImageSwitcher
        android:id="@+id/imageSwitcher1"
        android:layout_width="215dp"
        android:layout_height="254dp"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="0dp"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintHorizontal_bias="0.0" />

    <ImageSwitcher
        android:id="@+id/imageSwitcher2"
        android:layout_width="215dp"
        android:layout_height="308dp"
        android:layout_marginBottom="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginTop="8dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageSwitcher1"
        app:layout_constraintVertical_bias="1.0" />
</android.support.constraint.ConstraintLayout>
